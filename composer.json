{
    "name": "arimac/sigfox",
    "description": "A highlevel up-to-date complete Sigfox API client",
    "type": "library",
    "homepage": "https://arimacdev.github.io/php-sigfox-client",
    "license": "MIT",
    "authors": [
        {
            "name": "WhizSid",
            "email": "whizsid@aol.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Arimac\\Sigfox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arimac\\Sigfox\\Test\\": "tests/"
        }
    },
    "require": {
        "php":">=7.1"
    },
    "conflict":{
        "guzzlehttp/guzzle":"<7.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Required for using the builtin default HTTP client"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "4.x-dev"
    }
}
